{
  "name": "wb-tariffs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "knex": "KNEX_WORKING_DIR=. tsx ./node_modules/knex/bin/cli.js --knexfile src/config/knex/knexfile.ts",
    "knex:dev": "npm run knex -- --env development",
    "migrate:make": "npm run knex -- migrate:make",
    "migrate:latest": "npm run knex -- migrate:latest",
    "migrate:rollback": "npm run knex -- migrate:rollback",

    "build": "tsc",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js"
  },
  "imports": {
    "#*": [
      "./dist/*",
      "./src/*"
    ]
  },
  "keywords": [],
  "author": "",
