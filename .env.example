# ===============================
# Database (PostgreSQL)
# ===============================
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# ===============================
# Application
# ===============================
NODE_ENV=production
APP_PORT=3000

# ===============================
# Wildberries API
# ===============================
# Токен WB API (нужно получить отдельно)
WB_API_KEY=your_wb_api_key
WB_TARIFFS_URL=https://common-api.wildberries.ru/api/v1/tariffs/box

# ===============================
# Google Sheets
# ===============================
# ID таблиц через запятую, например:
# 1AbCdEfG123456,7GhIjKlM987654
GOOGLE_SHEETS_IDS=

# Сервисный аккаунт Google (файл credentials.json в base64)
GOOGLE_CREDENTIALS_JSON_BASE64=
